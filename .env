# .env.example
# Скопируйте этот файл в .env и заполните своими данными

# DonationAlerts OAuth приложение
# Получите эти данные на https://www.donationalerts.com/application/clients
DONATION_ALERTS_CLIENT_ID=your_client_id_here
DONATION_ALERTS_CLIENT_SECRET=your_client_secret_here

# Refresh Token для обновления access token
# Получите его через OAuth авторизацию
DONATION_ALERTS_REFRESH_TOKEN=your_refresh_token_here
